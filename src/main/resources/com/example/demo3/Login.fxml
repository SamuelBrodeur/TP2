        <Label fx:id="isConnected" layoutX="159.0" layoutY="271.0" prefHeight="15.0" prefWidth="65.0" text="Status" textFill="#cd1313">
            <font>
                <Font size="7.0" />
            </font>
        </Label>
      <TextField fx:id="username" layoutX="36.0" layoutY="119.0" promptText="Utilisateur" />
      <PasswordField fx:id="password" layoutX="36.0" layoutY="160.0" promptText="Mot de passe" />
      <Button layoutX="124.0" layoutY="238.0" mnemonicParsing="false" onAction="#Login" text="Connexion" />
      <Label fx:id="isSuccessful" layoutX="35.0" layoutY="195.0" prefHeight="43.0" prefWidth="179.0" textFill="#e80606" wrapText="true">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Text layoutX="41.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Connexion">
         <font>
            <Font size="32.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
